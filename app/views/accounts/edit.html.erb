<% logger.progname = "views/accounts" %>
<% logger.info "edit.htmlを読み込みました" %>

<% @page_title = "Edit my account" %>
<h1><%= @page_title %></h1>

<p><%= link_to "Return to my account", :account %></p>

<%= form_for @user, as: "account", url: :account do |form| %>
<%= render "users/form", form: form %>
<p><%= form.submit %></p>
<% end %>